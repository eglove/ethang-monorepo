{
  "author": "Ethan Glover",
  "devDependencies": {
    "@ethang/eslint-config": "^21.5.4",
    "eslint": "^9.23.0",
    "typescript-eslint": "^8.28.0"
  },
  "license": "ISC",
  "name": "ethang-monorepo",
  "pnpm": {
    "overrides": {
      "tailwindcss": "<4"
    },
    "onlyBuiltDependencies": [
      "@heroui/shared-utils",
      "@prisma/client",
      "@swc/core",
      "esbuild",
      "msw",
      "scrollmirror",
      "sharp",
      "workerd",
      "yarn"
    ]
  },
  "scripts": {
