{
  "name": "ethang-monorepo",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "all": "npm i -g pnpm && pnpm i && pnpm deps && pnpm test && pnpm lint && pnpm build",
    "deps": "pnpm up -r -i --latest && pnpm --filter eslint-config build",
    "lint": "pnpm -r lint",
    "build": "pnpm -r build",
    "test": "pnpm -r test"
  },
