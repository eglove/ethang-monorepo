{
  "$schema": "node_modules/wrangler/config-schema.json",
  "assets": {
    "not_found_handling": "single-page-application"
  },
  "compatibility_date": "2025-06-28",
  "compatibility_flags": ["nodejs_compat"],
  "d1_databases": [
    {
      "binding": "DB",
      "database_id": "<id>",
      "database_name": "<db_name>"
    }
  ],
  "main": "worker/index.ts",
  "name": "react-vite",
  "observability": {
    "enabled": true
  },
  "placement": {
    "mode": "smart"
  }
}
